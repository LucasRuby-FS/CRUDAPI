{
  "name": "apiWithCilent",
  "version": "1.0.0",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "install": "npm install --prefix api && npm install --prefix reactjs",
    "build": "npm run build --prefix reactjs",
    "start": "npm start --prefix api",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:react\"",
    "dev:api": "npm run dev --prefix api",
    "dev:react": "npm start --prefix reactjs"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
